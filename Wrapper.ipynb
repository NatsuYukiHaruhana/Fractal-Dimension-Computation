    "        return\n",
    "    lengthSide /= 3.0\n",
    "    snowflake(t, lengthSide, levels-1) \n",
    "    turtle.left(60) \n",
    "    snowflake(t, lengthSide, levels-1) \n",
    "    turtle.right(120) \n",
    "    snowflake(t, lengthSide, levels-1) \n",
    "    turtle.left(60) \n",
